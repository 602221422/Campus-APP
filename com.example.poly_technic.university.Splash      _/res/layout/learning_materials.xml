<?xml version="1.0" encoding="UTF-8"?>  
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"  
    android:id="@+id/empty_cart_view"  
    android:layout_width="fill_parent"  
    android:layout_height="fill_parent"  
    android:background="#ffefebe7" >  
  
    <RelativeLayout 
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:background="@drawable/top_bar_background"
		>
		<Button
			android:id="@+id/newsdetails_titlebar_previous"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerVertical="true"
			android:layout_marginLeft="3.0dip"
			android:layout_alignParentLeft="true" 
			android:background="@drawable/entertainment_previous_btn"
			/>
		<TextView
			android:id="@+id/newsdetails_titlebar_title"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="70.0dip"
			  android:layout_centerVertical="true"
			android:layout_toRightOf="@id/newsdetails_titlebar_previous"
			 android:textSize="20.0sp"
            android:textStyle="bold"
			android:textColor="@android:color/white"
			android:text="学习资料"
			android:gravity="center_horizontal"
			/>
	
	</RelativeLayout>	


        <LinearLayout  
            android:layout_width="fill_parent"  
            android:layout_height="wrap_content"  
            android:gravity="center_vertical"  
            android:orientation="vertical"  
            android:paddingBottom="60.0dip"  
            android:paddingLeft="10.0dip"  
            android:paddingRight="10.0dip"  
            android:paddingTop="10.0dip" >  

  
            <TextView  
                android:layout_width="fill_parent"  
                android:layout_height="wrap_content"  
                android:layout_marginBottom="10.0dip"  
                android:gravity="center_vertical"  
                android:paddingLeft="4.0dip"  
                android:text="学习网站"  
                android:textColor="#ff888888"  
                android:textSize="16.0sp" 
                android:layout_marginTop="45dp"/>  
  
            <LinearLayout  
                android:layout_width="fill_parent"  
                android:layout_height="wrap_content" >  
  
                <TableLayout  
                    android:id="@+id/MorePageTableLayout_Follow"  
                    android:layout_width="fill_parent"  
                    android:layout_height="wrap_content"  
                    android:paddingLeft="1.0dip"  
                    android:shrinkColumns="0"  
                    android:stretchColumns="0"
                    android:background="@drawable/shape3" >  
  
                    <TableRow  
                        android:id="@+id/more_page_row1"  
                        android:layout_width="fill_parent"  
                        android:layout_marginLeft="2.0dip"  
                        android:layout_marginRight="2.0dip"  
                        android:paddingBottom="16.0dip"  
                        android:paddingTop="16.0dip" >  
  
                        <TextView  
                            android:id="@+id/textbibei"
                            android:layout_width="wrap_content"  
                            android:layout_height="fill_parent"  
                            android:drawablePadding="10.0dip"  
                            android:gravity="center_vertical"  
                            android:includeFontPadding="false"  
                            android:paddingLeft="17.0dip"  
                            android:text="励志中国"  
                            android:textColor="#ff333333"  
                            android:textSize="16.0sp" />   
                    </TableRow>  
  
                    <TableRow  
                        android:id="@+id/more_page_row2"  
                        android:layout_width="fill_parent"  
                        android:layout_marginLeft="2.0dip"  
                        android:layout_marginRight="2.0dip"  
                        android:paddingBottom="16.0dip"  
                        android:paddingTop="8.0dip" >  
  
                        <TextView  
                            android:id="@+id/texthao123"
                            android:layout_width="wrap_content"  
                            android:layout_height="fill_parent"  
                            android:drawablePadding="10.0dip"  
                            android:gravity="center_vertical"  
                            android:includeFontPadding="false"  
                            android:paddingLeft="17.0dip"  
                            android:text="可可英语"  
                            android:textColor="#ff333333"  
                            android:textSize="16.0sp" />   
                    </TableRow>  
                </TableLayout>  
            </LinearLayout>   
            </LinearLayout>  

  
</RelativeLayout>  